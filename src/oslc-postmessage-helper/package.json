{
  "name": "@oslc/postmessage-helper",
  "version": "0.1.0",
  "type": "module",
  "description": "A TypeScript module for OSLC postMessage communication in delegated UIs",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "copy-to-demo": "npm run build && mkdir -p ../oslc-selection-demo/vendor/@oslc && cp -r dist ../oslc-selection-demo/vendor/@oslc/postmessage-helper",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "oslc",
    "postmessage",
    "delegated-ui",
    "iframe",
    "typescript"
  ],
  "author": "OSLC Community",
  "license": "EPL-1.0 OR EDL-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/oslc-op/refimpl.git",
    "directory": "src/oslc-postmessage-helper"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.1",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2"
  }
}
